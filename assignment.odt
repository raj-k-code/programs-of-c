//wAp to find out whether given year is leap year or not
#include<stdio.h>
int main(){
 int y;
 printf("enter year:\n");
 scanf("%d",&y);
 if(y%4==0)
 printf("year is leap: %d",y);
 else
 printf("year is not leap: %d",y);



return 0;
}


